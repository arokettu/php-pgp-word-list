{
    "name": "arokettu/pgp-word-list",
    "type": "library",
    "description": "PGP Word List Encoder/Decoder",
    "license": "MIT",
    "keywords": ["pgp word list", "encoding", "human-readable"],
    "homepage": "https://sandfox.dev/php/pgp-word-list.html",
    "authors": [
        {
            "name": "Anton Smirnov",
            "role": "developer",
            "homepage": "https://sandfox.me/",
            "email": "sandfox@sandfox.me"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/sandfox/php-pgp-word-list/-/issues",
        "source": "https://gitlab.com/sandfox/php-pgp-word-list",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\PgpWordList\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\PgpWordList\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "psy/psysh": "*",
        "sandfox.dev/code-standard": "^1.2025.09.17",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.18 || ^6"
    }
}
